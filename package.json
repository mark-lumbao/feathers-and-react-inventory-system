    "build:frontend": "webpack --progress --color --config=webpack.config.babel.js --env=production",
    "compile": "npm run build::config && shx rm -rf lib/ && tsc",
    "build::config": "node ./config-builder.js",
    "build:stats": "webpack --progress --color --profile --json --mode production --config=webpack.config.babel.js --env=production > stats.json",
    "hot": "webpack-dev-server --progress --color --config=webpack.config.babel.js --env=hot",
    "test": "npm run lint && npm run compile && npm run jest",
    "jest": "jest  --forceExit",
    "analyze": "webpack-bundle-analyzer stats.json",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix"
  },
  "standard": {
    "env": [
      "jest"
    ],
